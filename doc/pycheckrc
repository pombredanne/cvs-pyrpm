# Sample ~/.pycheckrc for newest pychecker from Fedora Core devel tree.

# maximum returns in a function
maxReturns = 40

# maximum # of arguments to a function
maxArgs = 15

# check if using implict and explicit return values
checkImplicitReturns = False

# No limit for the number of warnings/errors.
limit = 0

# max # of lines in a function
maxLines = 450

# max # of branches in a function
maxBranches = 80

# max # of local vars in a function
maxLocals = 90

